{
  "name": "iggy/iggy-php-client",
  "description": "Iggy PHP Client",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bartosz Lewandowski",
      "email": "bartosz.dev@yahoo.com"
    }
  ],
  "require": {
    "php": ">=8.2.0",
    "psr/http-client": "^1.0.3",
    "psr/http-factory": "^1.0.2",
    "psr/http-message": "^1.0.1",
    "psr/log": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/http-client": "^7.0",
    "nyholm/psr7": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Iggy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iggy\\Tests\\": "tests/"
    }
  }
}
